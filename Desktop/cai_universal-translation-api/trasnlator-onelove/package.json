{
  "name": "cai-universal-translation-api",
  "version": "1.0.0",
  "description": "Universal Translation API - ONE LOVE - 70+ languages support with AI Intelligence Network",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "build": "npm run build:packages",
    "build:packages": "npm run build:react && npm run build:nodejs && npm run build:python",
    "build:react": "cd packages/react && npm run build",
    "build:nodejs": "cd packages/nodejs && npm run build", 
    "build:python": "cd packages/python && python setup.py sdist bdist_wheel"
  },
  "keywords": [
    "translation",
    "multilingual",
    "i18n",
    "localization",
    "ai",
    "universal",
    "cai",
    "one-love"
  ],
